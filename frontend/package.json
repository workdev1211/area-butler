{
  "name": "redesign",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env TAILWIND_MODE=watch PORT=3001 craco start",
    "start-embed": "cross-env TAILWIND_MODE=watch PORT=3002 craco start --config craco-embed.config.js",
    "start-on-office": "cross-env TAILWIND_MODE=watch PORT=3003 craco start --config craco-on-office.config.js",
    "start-propstack": "cross-env TAILWIND_MODE=watch PORT=3004 craco start --config craco-propstack.config.js",
    "build": "craco build && mv build build-main",
    "build-embed": "craco build --config craco-embed.config.js && mv build build-embed",
    "build-on-office": "craco build --config craco-on-office.config.js && mv build build-on-office",
    "build-propstack": "craco build --config craco-propstack.config.js && mv build build-propstack",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
