  "scripts": {
    "copyfe": "mkdir -p dist/static/main && mkdir -p dist/static/embed && mkdir -p dist/static/on-office && mkdir -p dist/static/propstack && mkdir -p dist/static/my-vivenda && cp -R ../frontend/build-main/* dist/static/main && cp -R ../frontend/build-embed/* dist/static/embed && cp -R ../frontend/build-on-office/* dist/static/on-office && cp -R ../frontend/build-propstack/* dist/static/propstack && cp -R ../frontend/build-my-vivenda/* dist/static/my-vivenda",
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copyfe",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@area-butler-types/(.*)$": "<rootDir>/shared/types/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
